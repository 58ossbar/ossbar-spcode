/* packageOne/create/createktbx/createktbx.wxss */
.list_item {
  width: 100%;
  display: flex;
  flex-direction: row;
  margin-bottom: 20rpx;
}

.list_item .item_left {
  flex: 1;
  height: 160rpx;
  padding:20rpx;
  box-sizing: border-box;
}
.list_item .item_left .left_top {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 32rpx;
  font-weight: bold;
}

.list_item .item_left .left_bot {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  color: #959595;
}

.list_item .item_right {
  width: 200rpx;
  height: 160rpx;
  padding: 20rpx;
  box-sizing: border-box;
  overflow: hidden;
}

.key_name text {
  margin-left: 10rpx;
  color: red;
}